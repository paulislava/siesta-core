  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "prettier-check": "prettier --ignore-path ../.prettierignore --check '**/*.{ts,json}'",
    "prettier-fix": "prettier --ignore-path ../.prettierignore --write '**/*.{ts,json}'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nodemon -L --config nodemon-docker-debug.json",
    "start:prod": "node build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "typeorm": "",
    "typeorm:prod": "node node_modules/.bin/typeorm",
    "typeorm:wnd": "ts-node ./node_modules/typeorm/cli.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
