    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@template/shared": "file:../shared",
    "adminjs": "^5.10.3",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "config": "3.3.7",
